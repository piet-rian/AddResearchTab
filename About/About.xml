<?xml version="1.0" encoding="utf-8"?>
<ModMetaData>

    <packageId>piet.rian.art</packageId>
    <name>Add Research Tab</name>
    <authors>
        <li>piet-rian</li>
    </authors>
    <description>Move the researches added in some DLC  from the "Main" tab to the "(DLC name)" tab.</description>
    <supportedVersions>
        <li>1.6</li>
    </supportedVersions>

    <modVersion>1.0</modVersion>

    <modDependencies>

        <li>
            <packageId>Ludeon.RimWorld.Royalty</packageId>
            <displayName>Royalty</displayName>
        </li>
        <li>
            <packageId>Ludeon.RimWorld.Ideology</packageId>
            <displayName>Ideology</displayName>
        </li>
        <li>
            <packageId>Ludeon.RimWorld.Biotech</packageId>
            <displayName>Biotech</displayName>
        </li>
        <li>
            <packageId>Ludeon.RimWorld.Odyssey</packageId>
            <displayName>Odyssey</displayName>
        </li>
    </modDependencies>

    <loadAfter>
        <li>ludeon.rimworld</li>
        <li>ludeon.rimworld.royalty</li>
        <li>ludeon.rimworld.ideology</li>
        <li>ludeon.rimworld.biotech</li>
        <li>ludeon.rimworld.odyssey</li>
    </loadAfter>

</ModMetaData>
